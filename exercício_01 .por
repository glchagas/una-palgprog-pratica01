programa {
  cadeia cidadeMora
  cadeia cidadeEstuda

  funcao inicio() {
    escreva("Em qual cidade você mora?")
    leia(cidadeMora)
    escreva("Em qual cidade você estuda?")
    leia(cidadeEstuda)

    escreva("\n Você mora em:" + cidadeMora)
    escreva("\n Você estuda em:" + cidadeEstuda)
  }
}
